#!/usr/bin/env bash
set -eo pipefail

echo "---> Hello Moon buildpack"

env_dir=$2/env
layers_dir=$1
plan_path=$3

echo "     env_dir: ${env_dir}"
echo "     layers_dir: ${layers_dir}"
echo "     plan_path: ${plan_path}"
echo "     plan contents:"
cat ${plan_path} | sed 's/^/     /'

echo
echo "---> Done"
